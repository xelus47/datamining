             return true; 
           } 
           else
           {
             return true;
           }
        }
        function processOnEnterTab(field, evt, event_name) 
        { 
           var key; 
           
           if (window.event)
           {
              key = window.event.keyCode
           }
           else
           {
              key = evt.which;
           }
           
           if (key == 13 || key == 9)
           { 
             submitForm('form0', true, {'event':event_name}); 
             return true; 
           } 
           else
           {
             return true;
           }
